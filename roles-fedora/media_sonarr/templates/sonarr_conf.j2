  <UrlBase>{{ media_sonarr_basepath }}</UrlBase>
  <BindAddress>{{ media_sonarr_basepath }}</BindAddress>
  <SslPort>{{ media_sonarr_sslport }}</SslPort>
  <EnableSsl>{{ media_sonarr_enablessl }}</EnableSsl>
  <AuthenticationMethod>{{ media_sonarr_authmethod }}</AuthenticationMethod>
