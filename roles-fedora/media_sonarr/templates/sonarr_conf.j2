  <LaunchBrowser>{{ media_sonarr_launchbrowser }}</LaunchBrowser>
  <SslCertHash>{{ media_sonarr_sslcerthash }}</SslCertHash>
  <UpdateMechanism>{{ media_sonarr_updatemechanism }}</UpdateMechanism>
  <UpdateAutomatically>{{ media_sonarr_autoupdate }}</UpdateAutomatically>
